
      <div id="page-wrapper">
         <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header"  style="background:#99CCFF;height:80px;"><hr/>
                     &nbsp;&nbsp; &nbsp;&nbsp;ေငြစာရင္း
                       &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;<span ><%= link_to 'ေငြစာရင္းအသစ္ထည့္သြင္းရန္', new_money_uage_path, :class => "btn btn-default" %>
                      </span>
                    </h1>
            </div>
            <!-- /.col-lg-12 -->
         </div>
         <!-- /.row -->
         <div class="row">
            <div class="col-lg-12">
               <div class="panel panel-default">
                  <div class="panel-heading">
                     List of ေငြစာရင္း
                  </div>
                  <div class="panel-body">
                     <div class="row">
                        <div class="col-lg-12">
                           <h1>Lists of ေငြစာရင္း information</h1><br/>
                          
                          <table class="table">
                            <thead>
                              <tr>
                                <th>Date</th>
                                <th>ဝင္ေငြ</th>
                                <th>ထြက္ေငြ</th>
                                <th>ဆိုင္ရွိေငြ</th>
                                <th>Remark</th>
                                <th colspan="3"></th>
                              </tr>
                            </thead>

                            <tbody>
                              <% @money_uages.each do |money_uage| %>
                                <tr>
                                  <td><%= money_uage.transaction_date %></td>
                                  <td><%= money_uage.income %></td>
                                  <td><%= money_uage.outcome %></td>
                                  <td><%= money_uage.money_in_shop %></td>
                                  <td><%= money_uage.remark %></td>
                                  
                                  <td><%= link_to 'Edit', edit_money_uage_path(money_uage) %></td>
                                  <td><%= link_to 'Destroy', money_uage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                                </tr>
                              <% end %>
                            </tbody>
                          </table>
                           <br>
                           
                        </div>
                     </div>
                     <!-- /.row (nested) -->
                  </div>
                  <!-- /.panel-body -->
               </div>
               <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
         </div>
         <!-- /.row -->
      </div>
 


        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Edit Line Charts
                    </h1>

                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            List of Line chart data
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    
                                      <h1>List of Line chart data</h1>

                                      <table class="table table-bordered">
                                        <thead>
                                          <tr>
                                            <th>Month Name</th>
                                            <th>Year</th>
                                            <th>Total Income</th>
                                            <th colspan="3"></th>
                                          </tr>
                                        </thead>

                                        <tbody>
                                          <%  @line_charts.each do |line_chart| %>
                                            <tr>
                                              <td><%= line_chart.month_name %></td>
                                              <td><%= line_chart.year %></td>
                                              <td><%= line_chart.total_income %></td>
                                              <td><%= link_to 'Show', line_chart %></td>
                                              <td><%= link_to 'Edit', edit_line_chart_path(line_chart) %></td>
                                              <td><%= link_to 'Destroy', line_chart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                                            </tr>
                                          <% end %>
                                        </tbody>
									</table>


                                      <br>
                                     

                                </div>
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->









